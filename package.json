{
  "name": "ccc-transfer-helper",
  "version": "1.0.0",
  "description": "React TS, Python Django, and ",
  "main": "server.js",
  "scripts": {
    "install:client": "cd client && npm install",
    "install:server": "cd server && npm install",
    "install:all": "npm run install:client && npm run install:server",
    "start:client": "cd client && npm run dev",
    "start:server": "cd server && node server.js",
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "format": "prettier --write \"client/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" && prettier --write \"server/**/*.{js,json,md}\"",
    "format:client": "cd Client && npm run format",
    "format:server": "cd server && npm run format",
    "format:all": "npm run format:client && npm run format:server",
    "lint:client": "cd Client && npm run lint",
    "lint:server": "cd server && npm run lint",
    "lint:all": "npm run lint:client && npm run lint:server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.14.0",
    "@types/react-icons": "^3.0.0",
    "concurrently": "^8.2.2",
    "cypress": "^13.10.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "framer-motion": "^11.1.7",
    "react-icons": "^5.1.0",
    "styled-components": "^6.1.8",
    "tailwind-merge": "^2.3.0"
  }
}
